    "rxjs": "^6.5.4",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.6",
    "@nestjs/testing": "^7.6.5",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.20",
    "@types/node": "^13.9.1",
